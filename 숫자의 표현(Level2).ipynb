   "source": [
    "cnt = 1\n",
    "j=1\n",
    "\n",
    "while j != n:\n",
    "    result = 0\n",
    "    for i in range(j, n+1):\n",
    "        result += i\n",
    "        if result > n:\n",
    "            break\n",
    "        elif result == n:\n",
    "            cnt += 1\n",
    "            break\n",
    "    j += 1"
   ]
